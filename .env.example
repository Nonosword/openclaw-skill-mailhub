# MailHub local secrets template
# Copy this file to .env and fill real values.
# Never commit .env.

# Optional: override runtime state directory
MAILHUB_STATE_DIR=$HOME/.openclaw/state/mailhub

# Google OAuth
GOOGLE_OAUTH_CLIENT_ID=
GOOGLE_OAUTH_CLIENT_SECRET=

# Microsoft OAuth
MS_OAUTH_CLIENT_ID=
MS_OAUTH_CLIENT_SECRET=

# Optional: Google OAuth callback settings
MAILHUB_GOOGLE_CALLBACK_PORT=
MAILHUB_GOOGLE_OAUTH_TIMEOUT=300

# Optional: OpenClaw agent bridge (A-scheme)
MAILHUB_USE_OPENCLAW_AGENT=0
MAILHUB_STANDALONE_AGENT_CMD=
MAILHUB_AGENT_TIMEOUT=45
MAILHUB_MODE=openclaw
MAILHUB_OPENCLAW_JSON_PATH=~/.openclaw/openclaw.json
